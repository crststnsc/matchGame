<Window x:Class="matchGame.MatchingGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:matchGame"
        mc:Ignorable="d"
        Title="MatchingGameWindow" Height="670" Width="615">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBlock x:Name="timerTextBlock" Text="00:00" FontSize="20" FontWeight="Bold" Margin="10,10,0,0" />
        <Grid x:Name="gameBoard" Grid.Row="1">
        </Grid>
    </Grid>
</Window>
